<?xml version="1.0" encoding="utf-8" ?>

<resources>

  <style name ="dialog_animation">
    <item name="android:windowEnterAnimation">@anim/slide_up</item>
    <item name="android:windowExitAnimation">@anim/slide_down</item>
  </style>

  <style name ="navigation_drawer_theme" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">#00FF33</item>
    <item name="drawerArrowStyle">@style/MyArrowStyle</item>
  </style>

  <style name="MyArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
    <item name="colorPrimary">#F5F5F5</item>
    <item name="spinBars">true</item>
  </style>
</resources>
